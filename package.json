{
  "name": "@thanasisale/geotiff-downloader",
  "version": "1.0.0",
  "main": "dist/index.js",
  "bin": {
    "geotiff-downloader": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "pkg": {
    "targets": [
      "node20-linux-x64",
      "node20-macos-x64",
      "node20-win-x64"
    ],
    "outputPath": "executables"
  },
  "keywords": [
    "geotiff",
    "geoserver",
    "gis",
    "download",
    "cli",
    "geospatial"
  ],
  "author": "Thanasis Alexandropoulos @thanasisale",
  "license": "ISC",
  "description": "CLI tool for downloading GeoTIFF layers from a GeoServer",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    ".env.example"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thanasisale/geotiff-downloader.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/thanasisale/geotiff-downloader/issues"
  },
  "homepage": "https://github.com/thanasisale/geotiff-downloader#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.0",
    "dotenv": "^17.2.2",
    "fs-extra": "^11.3.1",
    "inquirer": "^12.9.4",
    "typescript": "^5.9.2",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.3.1",
    "@types/xml2js": "^0.4.14",
    "ts-node": "^10.9.2"
  }
}
